{
  "name": "escola-on",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@9.12.3",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare && pnpm run env && pnpm run db",
    "install-playwright": "playwright install --with-deps chromium",
    "test": "node ./scripts/run-test.js",
    "all": "pnpm install && pnpm run build && pnpm run preview",
    "env": "node scripts/env-create.mjs",
    "db": "pnpm run db:down && pnpm run db:up && pnpm run db:migrate && pnpm run db:seed",
    "db:up": "node scripts/db-create.mjs",
    "db:down": "node scripts/db-destroy.mjs",
    "db:seed": "node scripts/db-seed.mjs",
    "db:migrate": "prisma migrate dev --name init",
    "db:reset": "prisma migrate reset --force",
    "db:studio": "prisma studio"
  },
  "dependencies": {
    "@nuxt/fonts": "^0.10.2",
    "datatables.net-buttons-dt": "^3.1.2",
    "datatables.net-responsive-dt": "^3.0.3",
    "datatables.net-searchbuilder-dt": "^1.8.1",
    "datatables.net-select-dt": "^2.1.0",
    "datatables.net-vue3": "^3.0.2",
    "jszip": "^3.10.1",
    "nuxt": "^3.13",
    "radix-vue": "^1.9.9",
    "tailwind-variants": "^0.2.1",
    "vue": "^3.5.12",
    "vue-router": "^4.4.5",
    "vue-sonner": "^1.2.5"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.4.0",
    "@nuxt/icon": "^1.7.2",
    "@nuxt/test-utils": "^3.14.4",
    "@nuxtjs/color-mode": "^3.5.2",
    "@nuxtjs/tailwindcss": "^6.12.2",
    "@prisma/client": "^5.22.0",
    "@prisma/nuxt": "^0.0.35",
    "@tailwindcss/forms": "^0.5.9",
    "@vue/test-utils": "^2.4.6",
    "@vueuse/core": "^11.2.0",
    "@vueuse/nuxt": "^11.2.0",
    "happy-dom": "^15.11.1",
    "playwright-core": "^1.48.2",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "prisma": "5.22.0",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.6.3",
    "vitest": "^2.1.4",
    "vitest-environment-nuxt": "^1.0.1"
  }
}
